{
    "name": "trinity/framework",
    "description": "Trinity Framework",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "ME",
            "email": "info@modpreneur.com"
        }
    ],
    "require": {
      "php": ">=5.6",
      "symfony/symfony": "~2.8",
      "knplabs/doctrine-behaviors": "~1.2",
      "dmishh/settings-bundle": "2.0.*@dev",
      "doctrine/orm": "~2.5",
      "doctrine/doctrine-bundle": "~1.6",
      "apy/datagrid-bundle": "~2.2",
      "friendsofsymfony/user-bundle": "dev-master",
      "symfony/monolog-bundle": "~2.8"
    },

    "require-dev": {
      "sensio/generator-bundle": "~3.0",
      "phpunit/phpunit": "~5.0",
      "braincrafted/testing-bundle": "@stable"
    },

    "autoload": {
      "classmap": ["FrameworkBundle/"]
    },

    "minimum-stability": "stable",
    "extra": {
      "branch-alias": {
        "dev-master": "1.0-dev"
      }
    }
}
