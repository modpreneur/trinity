{
    "name": "trinity/framework",
    "description": "Trinity Framework",
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "ME",
            "email": "info@modpreneur.com"
        }
    ],
    "require": {
      "php": ">=7",
      "symfony/symfony": "~2.8 | ~3.0",
      "friendsofsymfony/user-bundle": "dev-master",
      "knplabs/doctrine-behaviors": "~1.2",
      "doctrine/orm": "~2.5",
      "doctrine/doctrine-bundle": "~1.6",
      "doctrine/data-fixtures": "~1",
      "doctrine/doctrine-fixtures-bundle": "~2",
      "symfony/monolog-bundle": "~2.8",
      "aws/aws-sdk-php": "~3.11",
      "egulias/email-validator": "~1.2"
    },

    "require-dev": {
      "sensio/generator-bundle": "~3.0",
      "phpunit/phpunit": "~5.0",
      "braincrafted/testing-bundle": "@stable",
      "trinity/logger": "dev-master",
      "trinity/settings": "dev-master"
    },

    "autoload": {
      "classmap": ["FrameworkBundle/"]
    },

    "minimum-stability": "stable",
    "extra": {
      "branch-alias": {
        "dev-master": "1.0-dev"
      }
    }
}
